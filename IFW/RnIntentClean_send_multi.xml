<rules>
   <activity block="true" log="true">
    <intent-filter>
      <action name="android.intent.action.SEND_MULTIPLE" />
      <cat name="android.intent.category.DEFAULT" />
      <type name="*/*" />
    </intent-filter>
    <component equals="com.alensw.PicFolder/com.alensw.cloud.ShareToCloudActivity" />
    <or>
      <sender type="system" />
      <not>
        <sender type="userId" />
      </not>
    </or>
  </activity>
